    <view class="btn flex-row">
      <image class='icon-25 ' src='{{uploadpath}}resource/{{res.huishou}}'></image>
      <view class="margin-left-10">可回收物</view>
    </view>
    <view class="btn flex-row">
      <image class='icon-25 ' src='{{uploadpath}}resource/{{res.youhai}}'></image>
      <view class="margin-left-10">有害垃圾</view>
    </view>
  </view>
  <view class="flex-row flex-column">
    <view class="txt-black h2">深圳市垃圾分类统考</view>
    <view class="txt-red h3">分</view>
  </view>
  <view class="biao">
    <view class="flex-row bg-gray">
      <view class="txt-black margin-left-70 txt-bold">题目</view>
      <view class="flex-1"></view>
      <view class="right"></view>
      <view class="txt-black margin-right-40 txt-bold">我的答案</view>
