    </view>
    <view class="flex-row ">
      <view class="margin-left-20">1</view>
      <view class="txt-black margin-left-30">题目</view>
      <view class="flex-1"></view>
      <view class="txt-black margin-right-40">我的答案</view>
      <view class="txt-black margin-right-30">是否正确</view>
    </view>
    <view class="flex-row bg-gray">
      <view class="margin-left-20">2</view>
      <view class="txt-black margin-left-30">题目</view>
      <view class="flex-1"></view>
      <view class="txt-black margin-right-40">我的答案</view>
      <view class="txt-black margin-right-30">是否正确</view>
    </view>
  </view>
</view>

<view class="flex-row ">
  <view class="flex-column margin-left-50">
    <image class='icon  margin-left-70' src='{{uploadpath}}resource/{{res.baocun}}' bindtap="bindsousuo"></image>
    <view class="txt-black">保存成绩单</view>
  </view>
 
  <view class="flex-column margin-left-70">
    <image class='icon ' src='{{uploadpath}}resource/{{res.zaici}}' bindtap="bindsousuo"></image>
    <view class="txt-black">再考一次</view>
  </view>
   
  <view class="flex-column margin-right-50">
    <image class='icon margin-left-15 ' src='{{uploadpath}}resource/{{res.share}}' bindtap="bindsousuo"></image>
    <view class="txt-black">分享</view>
  </view>
</view>