<view class="box">
  <view class="flex-row">
    <view class="margin-left-30 margin-top-30 txt-black h5">题目</view>
    <view class="flex-1"></view>
    <view class="margin-top-30 txt-lv h5">1</view>
    <view class="margin-top-30 margin-right-30 h5">/10</view>
  </view>
  <view class="flex-row flex-column">
    <view class="h3 txt-black">烫伤膏</view>
    <view class="txt-black margin-top-20">烫伤膏</view>
  </view>
  <view class="">
    <view class="margin-left-30 margin-top-30 margin-bottom-20">请选择：</view>
    <view class="btn flex-row">
      <image class='icon-25 ' src='{{uploadpath}}resource/{{res.chuyu}}'></image>
      <view class="margin-left-10">厨余垃圾</view>
    </view>
    <view class="btn flex-row">
      <image class='icon-25 ' src='{{uploadpath}}resource/{{res.qita}}'></image>
      <view class="margin-left-10">其他垃圾</view>
